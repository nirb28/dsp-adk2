# LLM Configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-4
LLM_API_KEY=your-api-key-here
LLM_BASE_URL=https://api.openai.com/v1
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2000

# Optional request param controls
# LLM_REQUEST_PARAM_ALLOWLIST=temperature,max_tokens
# LLM_REQUEST_PARAM_DENYLIST=max_completion_tokens
# LLM_REQUEST_PARAM_RENAME={"max_tokens": "max_completion_tokens"}
# LLM_REQUEST_PARAM_RENAME={"max_tokens":"max_completion_tokens","temperature":"temp","top_p":"topP"}
# LLM_SEND_ADDITIONAL_PARAMS=false

# LLM API key expansion example
# NVIDIA_API_KEY=your-nvidia-api-key
# LLM_API_KEY=${NVIDIA_API_KEY}

# Google ADK (Gemini) Configuration (use LLM_* values)
# LLM_API_KEY=your-google-api-key
# LLM_MODEL=gemini-2.5-flash

# Application Configuration
APP_HOST=0.0.0.0
APP_PORT=8200

# Data Storage
DATA_DIR=./data
AGENTS_DIR=./data/agents
TOOLS_DIR=./data/tools

# Security (optional)
API_KEY=your-admin-api-key
JWT_SECRET=your-jwt-secret

# Logging
LOG_LEVEL=INFO
AUTO_APP_RELOAD=false
DEBUG_TRACE=false
SSL_VERIFY=true

# Langfuse (optional)
LANGFUSE_ENABLED=false
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key
LANGFUSE_SECRET_KEY=your-langfuse-secret-key
LANGFUSE_HOST=https://cloud.langfuse.com
